{
  "name": "pinia-plugin-persistedstate",
  "type": "module",
  "version": "4.0.0-alpha.3",
  "description": "Configurable persistence and rehydration of Pinia stores.",
  "author": "Sacha Bouillez <hi@praz.dev> (https://praz.dev)",
  "license": "MIT",
  "repository": "github:prazdevs/pinia-plugin-persistedstate",
  "keywords": [
    "vue",
    "store",
    "pinia",
    "persistence",
    "pinia-plugin",
    "nuxt",
    "nuxt-module"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt/types.d.mts",
      "import": "./dist/nuxt/module.mjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:plugin && pnpm build:module",
    "build:plugin": "tsup src/index.ts --clean --dts --format esm",
    "build:module": "nuxt-module-build build --outDir dist/nuxt",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.0",
    "deep-pick-omit": "^1.2.0",
    "destr": "^2.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.1",
    "@nuxt/devtools": "^1.3.14",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.13.0",
    "@nuxt/test-utils": "^3.14.1",
    "@pinia/nuxt": "^0.5.4",
    "@types/node": "^22.5.0",
    "changelogen": "^0.5.5",
    "eslint": "^9.9.1",
    "jiti": "^1.21.6",
    "nuxt": "^3.13.0",
    "tsup": "^8.2.4",
    "typescript": "latest",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  }
}
